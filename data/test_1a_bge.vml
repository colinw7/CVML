        mov  .1,r0
        mov  .1,r1
        cmp  r0,r1
        bge  true
        brn  false
        halt

true:   mov .true_str,r0
        call sp,print_str
        halt

true_str: "true\n\0"

false:  mov .false_str,r0
        call sp,print_str
        halt

false_str: "false\n\0"

